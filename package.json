{
  "name": "vscode-net-tools",
  "displayName": "Network Tools",
  "description": "Network tools, including pcap and pcapng file parser and viewer",
  "version": "1.1.0",
  "publisher": "CusanzaBros",
  "repository": "https://github.com/CusanzaBros/vscode-net-tools",
  "license": "MIT",
  "icon": "media/icon.png",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "keywords": ["pcap", "pcapng", "network", "pktmon", "wireshark"],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "panel": [
        {
          "id": "packetDetails",
          "title": "Packet Details",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "packetDetails": [
        {
          "id": "packetDetails.detailsView",
          "name": "Packet Details",
          "type": "webview"
        }
      ]
    },
    "customEditors": [
			{
				"viewType": "packetreader.pcap",
				"displayName": "pcap file",
				"selector": [
					{
						"filenamePattern": "{*.pcap,*.pcapng}"
					}
				]
			}
		]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "lint": "eslint \"src/**/*.ts\""
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "esbuild": "^0.21.5",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  },
  "dependencies": {
    "ip-address": "^9.0.5"
  }
}
